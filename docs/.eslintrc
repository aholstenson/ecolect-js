{
  "parser": "babel-eslint",
  "root": true,
	"plugins": [
	],
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": true
    },
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:import/warnings"
	],
	"rules": {
		"no-console": [ "error", { "allow": [ "warn", "error" ] }]
  }
}
